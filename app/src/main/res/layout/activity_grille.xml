<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_grille"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.belhomme.alexandre.sudokuandroid.GrilleActivity">
    <TextView
        android:id="@+id/id_grille"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="TextView" />

    <view
        android:id="@+id/grille_draw"
        class="com.belhomme.alexandre.sudokuandroid.composants.GrilleDraw"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
</RelativeLayout>
